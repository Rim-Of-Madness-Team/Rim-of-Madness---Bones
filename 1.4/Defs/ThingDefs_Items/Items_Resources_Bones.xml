<Defs>

  <ThingDef ParentName="ResourceVerbBase">
    <defName>BoneItem</defName>
    <label>bone</label>
    <description>Bones from a once living thing. Useful for building structures or weapons. A bone can be used as a weapon in a pinch.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Bone</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.0</MarketValue>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>0.25</Mass>
      <Flammability>0.0</Flammability>
      <StuffPower_Armor_Sharp>0.54</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.54</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>0.9</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>8</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>4</StuffPower_Insulation_Heat>
      <MeleeWeapon_CooldownMultiplier>2.0</MeleeWeapon_CooldownMultiplier>
      <SharpDamageMultiplier>0.8</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.3</BluntDamageMultiplier>
    </statBases>
    <stuffProps>
      <stuffAdjective>bone</stuffAdjective>
      <categories>
        <li>Woody</li>
      </categories>
      <commonality>1.00</commonality>
      <color>(175,175,175)</color>
      <constructEffect>ConstructWood</constructEffect>
      <appearance>Planks</appearance>
      <soundImpactStuff>BulletImpact_Wood</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
      <statFactors>
        <MaxHitPoints>0.4</MaxHitPoints>
        <Beauty>1</Beauty>
        <Flammability>0</Flammability>
        <WorkToMake>0.7</WorkToMake>
        <WorkToBuild>0.7</WorkToBuild>
        <DoorOpenSpeed>1.2</DoorOpenSpeed>
        <MeleeWeapon_CooldownMultiplier>0.9</MeleeWeapon_CooldownMultiplier>
      </statFactors>
    </stuffProps>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <tools>
      <li>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>4</power>
        <cooldownTime>2.2</cooldownTime>
      </li>
    </tools>
  </ThingDef>
  
